loadstring(game:HttpGet("https://pastebin.com/raw/nKCdGTgZ"))()

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

-- starting notications --

OrionLib:MakeNotification({
	Name = "krobar",
	Content = "krobar has been executed",
	Image = "rbxassetid://4483345998",
	Time = 5
})

OrionLib:MakeNotification({
	Name = "Join the discord!",
	Content = "Get access to our update log and more features! Link on main page of UI",
	Image = "rbxassetid://4483345998",
	Time = 5
})

-- config save --

local MainWindow = OrionLib:MakeWindow({Name = "krobar", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionFolder"})

-- Info Tab --

local InfoTab = MainWindow:MakeTab({
	Name = "Info",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

InfoTab:AddParagraph("Version","krobar version 1.1")
InfoTab:AddParagraph("Credits","&User (Creator), Rifleman (Tester)")
InfoTab:AddParagraph("Discord","https://discord.gg/5TSZ5utgjk")
InfoTab:AddParagraph("Updates","Added Google Translate Script + Translation Tab")

-- Main Scripts --

local MainTab = MainWindow:MakeTab({
	Name = "Main Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

-- Translator --

MainTab:AddLabel("Translators")

MainTab:AddButton({
	Name = "Google Translate",
	Callback = function()
      		loadstring(game:HttpGet("https://pastebin.com/raw/7fEXTbBT"))();
  	end    
})

-- Admin scripts --

