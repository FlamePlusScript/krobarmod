-- Setings tab --

local SettingsTab = MainWindow:MakeTab({
	Name = "Settings",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

SettingsTab:AddButton({
	Name = "Re-Execute",
	Callback = function()
